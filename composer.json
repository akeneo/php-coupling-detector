{
    "name": "akeneo/php-coupling-detector",
    "description": "PHP coupling detector",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Dupont <nicolas@akeneo.com>",
            "homepage": "http://www.akeneo.com"
        },
        {
            "name": "Julien Janvier <j.janvier@gmail.com>"
        }
    ],
    "require": {
        "php": ">=5.3.6",
        "friendsofphp/php-cs-fixer": "~1.11",
        "symfony/event-dispatcher": "~2.3|~3.0",
        "symfony/finder": "~2.3|~3.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/filesystem": "~2.3|~3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Akeneo\\CouplingDetector\\": "src/Akeneo/CouplingDetector/"
        }
    },
    "bin": ["bin/php-coupling-detector"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
